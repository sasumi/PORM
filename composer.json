  "homepage": "http://blog.yikepi.com/",
  "type": "library",
  "license": "MIT",
  "authors": [
	{
	  "name": "Sasumi.H",
	  "email": "cnsasumi@outlook.com",
	  "homepage": "http://blog.yikepi.com/"
	}
  ],
  "keywords": [
	"ORM",
	"DatabaseModel"
